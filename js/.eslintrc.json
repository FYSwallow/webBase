{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12 //可支持的es版本
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "eqeqeq": 0,
        "indent": ["error", "tab"]
    }
}
